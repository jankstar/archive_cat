{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "archive_cat",
  "mainBinaryName": "archive_cat",
  "version": "0.1.0",
  "identifier": "de.jankstar",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ArchivCat",
        "minWidth": 1200,
        "width": 1200,
        "minHeight": 900,
        "height": 900
      }
    ],
    "withGlobalTauri": false,
    "security": {
      "csp": null,
      "pattern": {
        "use": "brownfield"
      }
    }
  }
}